lookup akhn_conditional_stacking {

    ignore sub k1' xx' t1' [r1 r2 l1 l2];
    ignore sub k1' xx' t1' xx t1;
    sub k1' xx' t1'  by k1t1;

    ignore sub k1' xx' n1' [r1 r2 l1 l2];
    ignore sub k1' xx' n1' xx [n1 r3];
    sub k1' xx' n1'  by k1n1;

    ignore sub k1' xx' m1' [r1 r2 l1 l2];
    ignore sub k1' xx' m1' xx [r3 l3];
    sub k1' xx' m1'  by k1m1;

    ignore sub k1' xx' sh' xx' [nh m1]' [r1 r2 l1 l2];
    ignore sub k1' xx' sh' xx' [nh m1]' xx r3;
    sub k1' xx' sh' xx' m1' by k1shm1;
    sub k1' xx' sh' xx' nh' by k1shnh;

    ignore sub k1' xx' rh' xx' rh' [r1 r2 l1 l2];
    sub k1' xx' rh' xx' rh' by k1rhrh;

    ignore sub k3' xx' k4' [r1 r2 l1 l2];
    ignore sub k3' xx' k4' xx r3;
    sub k3' xx' k4' by k3k4;

    ignore sub k4' xx' n1' [r2 l1 l2];
    ignore sub k4' xx' n1' xx r3;
    sub k4' xx' n1' by k4n1;

    ignore sub ch1' xx' ch2' [r2 l1 l2];
    sub ch1' xx' ch2' by ch1ch2;

    ignore sub ch3' xx' nj' [u1 u2 r1 r2 l1 l2];
    sub ch3' xx' nj' by ch3nj;

    ignore sub nj' xx' ch3' [r1 r2 l1 l2];
    sub nj' xx' ch3' by njch3;

    ignore sub t3' xx' t3' [r1 r2 l1 l2];
    ignore sub t3' xx' t3' xx r3;
    sub t3' xx' t3' by t3t3;

    ignore sub t3' xx' t4' [r1 r2 l1 l2];
    ignore sub t3' xx' t4' xx r3;
    sub t3' xx' t4'  by t3t4;

    ignore sub nh' xx' t4' [u1 u2 r1 r2 l1 l2];
    ignore sub nh' xx' t4' xx r3;
    sub nh' xx' t4' by nht4;

    ignore sub nh' xx' m1' [r1 r2 l1 l2];
    ignore sub nh' xx' m1' xx r3;
    sub nh' xx' m1' by nhm1;

    ignore sub th1' xx' m1' [r2 l1 l2];
    ignore sub th1' xx' m1' xx r3;
    sub th1' xx' m1' by th1m1;

    ignore sub th1' xx' s1' xx' n1' [r1 r2 l1 l2];
    ignore sub th1' xx' s1' xx' n1' xx r3;
    sub th1' xx' s1' xx' n1' by th1s1n1;

    ignore sub th1' xx' s1' xx' m1' [r1 r2 l1 l2];
    ignore sub th1' xx' s1' xx' m1' xx r3;
    sub th1' xx' s1' xx' m1' by th1s1m1;

    ignore sub p1' xx' th1' [u1 u2 r1 r2 l1 l2];
    ignore sub p1' xx' th1' xx [r3 th1];
    sub p1' xx' th1' by p1th1;

    ignore sub p1' xx' p2' [u1 u2 r1 r2 l1 l2];
    ignore sub p1' xx' p2' xx r3;
    sub p1' xx' p2' by p1p2;

    ignore sub p1' xx' s1' [u1 u2 r1 r2 l1 l2];
    ignore sub p1' xx' s1' xx [r3 s1 l3];
    sub p1' xx' s1'  by p1s1;

    ignore sub p3' xx' th3' [u1 u2 r1 r2 l1 l2];
    ignore sub p3' xx' th3' xx [r3 th3];
    sub p3' xx' th3'  by p3th3;

    ignore sub y1' xx' k1' [r1 r2 l1 l2];
    ignore sub y1' xx' k1' xx [r3 l3];
    ignore sub y1' xx' k1' xx k1 [r1 r2 l1 l2];
    ignore sub y1' xx' k1' xx k1 xx [r3 l3];
    sub y1' xx' k1' by y1k1;

    ignore sub y1' xx' th1' [u1 u2 r1 r2 l1 l2];
    ignore sub y1' xx' th1' xx [r3 th1];
    sub y1' xx' th1' by y1th1;

    ignore sub l3' xx' k1' [r2 l1 l2];
    ignore sub l3' xx' k1' xx [k1 l3];
    sub l3' xx' k1' by l3k1;

    ignore sub z1' xx' m1' [u1 u2 r1 r2 l1 l2];
    ignore sub z1' xx' m1' xx [r3 m1];
    sub z1' xx' m1' by z1m1;

    ignore sub z1' xx' z1' [r1 r2 l1 l2];
    ignore sub z1' xx' z1' xx r3;
    sub z1' xx' z1' by z1z1;

    ignore sub sh' xx' k1' [r2 l1 l2];
    ignore sub sh' xx' k1' xx [k1 l3];
    ignore sub sh' xx' k1' xx' r3' [u1 u2 r1 r2 l1 l2];
    sub sh' xx' k1' by shk1;

    ignore sub sh' xx' [t1 nh]' xx [t1 nh l3];
    ignore sub sh' xx' t1' xx' r3' [u1 u2 r1 r2 l1 l2];
    ignore sub sh' xx' [t1 t2]' [r1 r2 l1 l2];
    sub sh' xx' t1' by sht1;
    sub sh' xx' t2' by sht2;
    sub sh' xx' nh' by shnh;

    ignore sub sh' xx' p1' [u1 u2 r1 r2 l1 l2];
    ignore sub sh' xx' p1' xx [p1 r3 l3];
    sub sh' xx' p1' by sh1p1;

    ignore sub sh' xx' m1' [u1 u2 r1 r2 l1 l2];
    ignore sub sh' xx' m1' xx [m1 r3 l3];
    sub sh' xx' m1' by shm1;

    ignore sub s1' xx' [m1 n1]' [u1 u2 r1 r2 l1 l2];
    ignore sub s1' xx' p1' [r1 r2 l1 l2];
    ignore sub s1' xx' m1' xx [m1 r3 l3];
    ignore sub s1' xx' n1' xx [n1 r3 l3];
    ignore sub s1' xx' p1' xx [p1 r3 l3];
    ignore sub s1' xx' k1' xx l3;
    sub s1' xx' k1' by s1k1;
    sub s1' xx' m1' by s1m1;
    sub s1' xx' n1' by s1n1;
    sub s1' xx' p1' by s1p1;

    ignore sub s1' xx' th1' [r2 l1 l2];
    ignore sub s1' xx' th1' xx [th1 l3];
    ignore sub s1' xx' th1' xx' r3' [u1 u2 r1 r2 l1 l2];
    sub s1' xx' th1' by s1th1;

    ignore sub s1' xx' s1' [r1 r2 l1 l2];
    ignore sub s1' xx' s1' xx [r3 l3];
    sub s1' xx' s1' by s1s1;

    #ignore sub s1' xx' rh' xx' rh' xx r3;
    #sub s1' xx' rh' xx' rh'  by s1rhrh;

    ignore sub zh' xx' [k1 ch1]' [r1 r2 l1 l2];
    ignore sub zh' xx' k1' xx [k1 r3 l3];
    ignore sub zh' xx' ch1' xx [ch1 r3 l3];
    sub zh' xx' k1'  by zhk1;
    sub zh' xx' ch1'  by zhch1;

} akhn_conditional_stacking;


